/*
# include <stdio.h>
int main()
{
	int i,j,k,a;
	int n;
	printf("enter number ");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=n-i;j++)
	    {
		     printf(" ");
	    }
	    
	   for(k=1,a=65;k<=i;k++,a++)
	   {
		    printf("%c",a);
	   }
	   
	    printf("\n");
	}	
}  
*/

/*
