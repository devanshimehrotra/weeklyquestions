/*
# include <stdio.h>
int main()
{
	int i,j,k;
	int n;
	printf("enter number ");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=n;j++)
	     {
		    if (i==1||j==1||i==n||j==n)
		    {
		    	printf("* ");
			}
			else 
			   printf("  ");
	     }
	     printf("\n");
	}
}  
*/

