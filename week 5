/*
# include <stdio.h>
int main()
{
	int i,j,num=1;
	int n;
	printf("enter number ");
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=i;j++)
	    {
		     printf("%d",num);
		     num++;
	    }
	    printf("\n");
	}
} 
*/

/*
h. 1
   10
   101
   1010
   10101
*/

/*
# include <stdio.h>
int main()
{
	int i,j,num=1;
	int n;
	printf("enter number ");
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	{
		for(j=1;j<=i;j++)
	    {
		    if (j%2==0)
		      printf("0");
		    else 
		      printf("1");
	    }
	    printf("\n");
	}
} 
*/

/*
i. 5
   54
   543
   5432
   54321
*/

/*
# include <stdio.h>
int main()
{
	int i,j,k;
	int n;
	printf("enter number ");
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	{
		for(j=1,k=n;j<=i;j++,k--)
	    {
		     printf("%d",k);
	    }
	    printf("\n");
	}
}  
*/

